{
    "version": 2,
    "builds": [
      {
        "src": "battlefield4-server-info/package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "battlefield4-backend/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "battlefield4-backend/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "battlefield4-server-info/build/$1"
      }
    ]
  }
  